{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "calebporzio/parental": "^1.0",
        "cardmonitor/cardmarket-api": "dev-master",
        "cardmonitor/skryfall-api": "dev-master",
        "facade/ignition": "^2.5",
        "fideloper/proxy": "^4.4",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^8.83",
        "laravel/legacy-factories": "^1.3",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^3.3",
        "lukeraymonddowning/honey": "^0.3.1",
        "mariuzzo/laravel-js-localization": "^1.5",
        "nemiah/php-fints": "^3.0",
        "nemiah/php-sepa-xml": "dev-master",
        "niklasravnsborg/laravel-pdf": "^4.1",
        "pusher/pusher-php-server": "^7.2",
        "socialiteproviders/dropbox": "^4.2",
        "spatie/flysystem-dropbox": "^1.2",
        "spatie/laravel-cookie-consent": "^2.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeds/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
